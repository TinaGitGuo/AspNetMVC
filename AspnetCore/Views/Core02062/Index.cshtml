@*@model IEnumerable<AspnetCore.Contact1>*@

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Company)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contact)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Recid)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
      
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Company)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contact)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Recid)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Accountno">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Accountno">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Accountno">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
